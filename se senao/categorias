programa {
  funcao inicio() {
    inteiro idade


    escreva("Digite a sua idade: ")
    leia(idade)


    se(idade >= 5 e idade <= 10){
      escreva("Infantil")
    }
    senao se(idade >= 11 e idade <= 15){
      escreva("Juvenil")
    }
    senao se(idade >= 16 e idade <= 20){
      escreva("Junior")
    }
    senao se(idade >= 21 e idade <= 25){
      escreva("Profissional")
    }
    senao {
      escreva("InvÃ¡lido")
    }
  }
}
