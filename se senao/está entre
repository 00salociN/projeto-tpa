programa {
  funcao inicio() {
    inteiro num


    escreva("Digite um múmero: ")
    leia(num)


    se(num >= 5 e num < 200){
      escreva("Está entre 5 a 199")
    } senao se(num >= 200 e num <= 400){
      escreva("Está entre 200 e 400")
    } senao se(num >= 500 e num <= 1000){
      escreva("Está entre 500 e 1000")
    } senao{
      escreva("Inválido")
    }
  }
}
